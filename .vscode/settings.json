{
  "eslint.validate": [
    {
      "language": "vue",
      "autoFix": true
    },
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "javascriptreact",
      "autoFix": true
    }
  ],
  "eslint.autoFixOnSave": true,
  "editor.formatOnSave": false,
  "vetur.validation.template": false,
  // ES Lint
  "eslint.options": {
    "configFile": ".eslintrc.js"
},
"editor.codeActionsOnSave": {
  "source.fixAll.eslint": "explicit"
},
"workbench.colorCustomizations": {
  "activityBar.activeBackground": "#5f1d04",
  "activityBar.activeBorder": "#057a25",
  "activityBar.background": "#5f1d04",
  "activityBar.foreground": "#e7e7e7",
  "activityBar.inactiveForeground": "#e7e7e799",
  "activityBarBadge.background": "#057a25",
  "activityBarBadge.foreground": "#e7e7e7",
  "statusBar.background": "#2e0e02",
  "statusBar.foreground": "#e7e7e7",
  "statusBarItem.hoverBackground": "#5f1d04",
  "titleBar.activeBackground": "#2e0e02",
  "titleBar.activeForeground": "#e7e7e7",
  "titleBar.inactiveBackground": "#2e0e0299",
  "titleBar.inactiveForeground": "#e7e7e799",
  "sash.hoverBorder": "#5f1d04",
  "statusBarItem.remoteBackground": "#2e0e02",
  "statusBarItem.remoteForeground": "#e7e7e7",
  "commandCenter.border": "#e7e7e799"
},
"peacock.color": "2e0e02",
}